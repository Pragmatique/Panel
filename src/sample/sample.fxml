                        <MenuItem mnemonicParsing="false" onAction="#setDoc" text="doc" />
                        <MenuItem mnemonicParsing="false" onAction="#setXls" text="xls" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="ParserMenu">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#parser" text="ParseTV" />
                        <MenuItem mnemonicParsing="false" onAction="#parserRadio" text="ParseRadio" />
                        <MenuItem mnemonicParsing="false" onAction="#detailedTVParser" text="DetailedTVParser" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="Merge">
                     <items>
                        <MenuItem mnemonicParsing="false" onAction="#merge" text="MergeFiles" />
                     </items>
                  </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
            <Label fx:id="MyLabel0" alignment="TOP_LEFT" centerShape="false" prefHeight="249.0" prefWidth="263.0" textFill="#ff0606" BorderPane.alignment="TOP_LEFT">
               <font>
                  <Font name="PT Sans" size="12.0" />
               </font></Label>
         </center>
      </BorderPane>
   </children>
</Pane>
