<Pane fx:id="maPane" prefHeight="275.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.Controller">
   <children>
      <BorderPane layoutX="-1.0" prefHeight="275.0" prefWidth="300.0">
         <top>
            <MenuBar prefHeight="25.0" prefWidth="128.0" BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="File&gt;&gt;">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#chooseDirectory" text="Choose Directory" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="FileType&gt;&gt;">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#setTxt" text="txt" />
